{
  "plugins": [
    "prettier-plugin-tailwindcss",
    "prettier-plugin-classnames",
    "prettier-plugin-merge"
  ],
  "tailwindConfig": "./tailwind.config.ts",
  "overrides": [
    {
      "files": "*.css",
      "options": {
        "printWidth": 80,
        "proseWrap": "always"
      }
    },
    {
      "files": "*.svg",
      "options": {
        "parser": "html",
        "printWidth": 80,
        "proseWrap": "always"
      }
    }
  ]
}
